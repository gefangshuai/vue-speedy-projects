{
    "name": "vue-speedy-antd",
    "version": "0.1.0",
    "private": false,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "serve:hot": "nodemon --exec yarn serve"
    },
    "dependencies": {
        "ant-design-vue": "^1.3.16",
        "core-js": "^2.6.5",
        "vue": "^2.6.10"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.11.0",
        "@vue/cli-service": "^3.11.0",
        "babel-plugin-import": "^1.12.1",
        "compression-webpack-plugin": "^3.0.0",
        "less": "^3.10.3",
        "less-loader": "^5.0.0",
        "nodemon": "^1.19.1",
        "vue-template-compiler": "^2.6.10"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "nodemonConfig": {
        "watch": [
            "conf/",
            "vue.config.js",
            "src/core/a-less-override.js"
